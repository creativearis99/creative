  "license": "ISC",
  "main": "dist/index.js",
  "engines": {
    "node": "10"
  },
  "private": true,
  "files": [
    "dist"
  ],
  "watch": {
    "fucntion": "dist/*.js"
  },
  "config": {
    "port": "3333",
    "trigger": "http",
    "entry": "httpRequest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativearis99/creative.git"
  },
  "scripts": {
    "build:watch": "rollup --config=../../../rollup.config.functions.js -w",
